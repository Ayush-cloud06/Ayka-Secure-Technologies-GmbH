{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DenyRootUsage",
            "Effect": "Deny",
            "Action": "*",
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "aws:PrincipalType": "Account"
                }
            }
        }
    ]
}